curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X GET http://localhost:8080/api/info

curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X GET http://localhost:8080/api/expenses

curl -H "Content-Type: application/json" -X POST -d '{"id": 1, "date": "2017-02-01", "amount": 123}' http://localhost:8080/api/expenses